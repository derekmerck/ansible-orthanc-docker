function OnStoredInstance(instanceId, tags, metadata)

    {%- for d in orthanc_destinations %}
        {%- if d.type == "dicom" %}

    SendToModality( instanceId, '{{ d.dest }}' )

        {%- elif d.type == "peer" %}

    SendToPeer( instanceId, '{{ d.dest }}' )

        {%- endif %}
    {%- endfor %}

    Delete(instanceId)
end